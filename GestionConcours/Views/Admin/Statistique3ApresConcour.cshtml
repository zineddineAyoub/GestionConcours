                    <td class="text-center">@ViewBag.statPerDiplomAtt.nbDeug</td>
                    <td class="text-center">@ViewBag.statPerDiplomAtt.nbLicencePro</td>
                    <td class="text-center">@ViewBag.statPerDiplomAtt.nbLicenceSt</td>
                    <td class="text-center">@ViewBag.statPerDiplomAtt.nbLicenceFnd</td>
                    <td class="text-center">@ViewBag.statPerDiplomAtt.getTotal()</td>


                </tr>


            </tbody>

        </table>

    </div>




</div>
<!-- /.container-fluid -->
