                        <td class="text-center">@r.nomFiliere</td>
                        <td class="text-center">@r.nbDut</td>
                        <td class="text-center">@r.nbDeug</td>
                        <td class="text-center">@r.nbLicencePro</td>
                        <td class="text-center">@r.nbLicenceSt</td>
                        <td class="text-center">@r.nbLicenceFnd</td>
                        <td class="text-center">@r.getTotal()</td>

                    </tr>
                }
                <tr></tr>
                <tr>
                    <td class="text-center">Total</td>
                    <td class="text-center">@ViewBag.statPerDiplomPrincipale.nbDut</td>
                    <td class="text-center">@ViewBag.statPerDiplomPrincipale.nbDeug</td>
                    <td class="text-center">@ViewBag.statPerDiplomPrincipale.nbLicencePro</td>
                    <td class="text-center">@ViewBag.statPerDiplomPrincipale.nbLicenceSt</td>
                    <td class="text-center">@ViewBag.statPerDiplomPrincipale.nbLicenceFnd</td>
                    <td class="text-center">@ViewBag.statPerDiplomPrincipale.getTotal()</td>


                </tr>





            </tbody>

        </table>

    </div>
    <div class="align-content-center">
        <h3 class="font-weight-bold text-center pt-3 pb-5">Statistiques des listes d'attente 3 ème année</h3>

    </div>
    <div>

        <table class="table table-bordered ">
            <tr class="table-danger">
                <th class="text-center"></th>
                <th class="text-center">DUT</th>
                <th class="text-center">DEUG</th>
