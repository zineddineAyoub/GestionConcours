                <th class="text-center">Licence ST</th>
                <th class="text-center">Licence fnd</th>
                <th class="text-center">Total</th>


            </tr>
            <tbody id="tbody">
                @foreach (var r in ViewData["statistiquesAttente"] as List<GestionConcours.ViewModels.Statistique>)
